.map{
  position: relative
}
.location{
  width: 80%;
  line-height: 100rpx;
  background: #ffffff;
  color: #666666;
  font-size: 35rpx;
  text-align: center;
  position: absolute;
  left: 10%;
  top: 10%;
  z-index: 999;
}
.location i{
  font-size: 45rpx;
}
.dw{
  position: absolute;
  left: 3%;
  bottom: 3%;
  width: 100rpx;
  text-align: center;
  line-height: 100rpx;
  color: #13aaf7;
  z-index: 999999999999999;
  height: 100rpx;
  background: #ffffff;
}
.dw i{
font-size: 60rpx;
}
.store{
  width: 80rpx;
  height: 80rpx;
  position: absolute;
  top: 60%;
  left: 40%;
  z-index: 999999999999999
}
.store image{
  width: 80rpx;
  height: 80rpx;
}